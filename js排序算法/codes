1.冒泡排序
  function bubble(arr){
      var len=arr.length;
      for(var i=0;i<len;i++){
          for(var j=0;j<len-1-i;j++){
              if(arr[j]>arr[j+1]){
                  var tmp=arr[j+1];
                  arr[j+1]=arr[j];
                  arr[j]=tmp;
              }
          }
      }
      return arr;
  }
